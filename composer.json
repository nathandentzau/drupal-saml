    "description": "SAML 2.0 integration for Drupal.",
    "type": "drupal-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Dentzau",
            "email": "nathan.dentzau@gmail.com"
        }
    ],
    "repositories": [
        {
          "type": "composer",
